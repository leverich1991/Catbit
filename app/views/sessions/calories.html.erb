<h1>Calories</h1>
	<div id="calories" style="height: 200px;"></div>
	<script>
	new Morris.Line({
	element: 'calories',
	data: [
    { day: '1', value: <%= @daily_activity['summary']['caloriesOut'] %> },
    { day: '2', value: 1800 },
    { day: '3', value: 1200 },
    { day: '4', value: 500 },
    { day: '5', value: 700 }
  ],
  xkey: 'day',
  parseTime: false,
  ykeys: ['value'],
  labels: ['Value'],
  lineColors: ['red']
});</script>